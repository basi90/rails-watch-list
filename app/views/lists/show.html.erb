<div class="container">
  <h1><%= @list.name %></h1>

  <h2>Bookmarks</h2>

  <ul>
    <% @list.bookmarks.each do |bookmark| %>
      <li>
        <%= bookmark.comment %>
      </li>
    <% end %>
  </ul>

  <%= link_to "NewBookmark", new_list_bookmark_path(@list), class: "btn btn-primary" %>
  <%= link_to "Back", lists_path, class: "btn btn-secondary" %>
</div>
